<mp-cells footer="{{footerts}}">
  <mp-cell ext-class="">
    <text slot="title">
      <slot name="sqName"></slot>
    </text>
    <switch style="transform: scale(0.8);" model:checked="{{ishide}}" slot="footer" />
  </mp-cell> 
  <!-- 图片上传 -->
  <mp-cell wx:if="{{ishide}}">
    <mp-uploader binddelete="deleteImg" bindfail="uploadError" bindsuccess="uploadSuccess" select="{{selectFile}}" upload="{{uplaodFile}}" files="{{files}}" max-count="4" title="{{title}}" tips="{{tips}}"></mp-uploader>
  </mp-cell>
</mp-cells>